# 1 Minute instance/container down rule.

groups:
- name: example
  rules:

  - alert: service_down
    expr: up == 0
    for: 1m
    labels:
      severity: high
    annotations:
      summary: "Instance {{ $labels.instance }} down"
      description: "{{ $labels.instance }} of job {{ $labels.job }} has been down for more than 1 minutes."